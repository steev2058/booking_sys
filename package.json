{
  "name": "baraka-branch-booking",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:up": "docker compose -f docker-compose.db.yml up -d",
    "db:down": "docker compose -f docker-compose.db.yml down",
    "db:schema": "mysql -h ${DB_HOST:-127.0.0.1} -P ${DB_PORT:-3306} -u ${DB_USER:-booking_user} -p${DB_PASS:-booking_pass} ${DB_NAME:-booking_sys} < database/schema.sql",
    "db:migrate": "node scripts/migrate-json-to-mysql.js"
  },
